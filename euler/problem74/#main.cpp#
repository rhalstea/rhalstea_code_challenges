#include <iostream>
#include <vector>
#include <sstream>
#include <cstdlib>

std::vector<int> get_digits(int val)
{
  std::vector<int> ret;
  std::stringstream tmp;
  std::string s;

  tmp << val;
  s = tmp.str();
  std::cout << s << ": ";
  for (int i = 0; i < tmp.str().size(); ++i)
    ret.push_back( (int)(s[i] - 48) );

  return ret;
}

int fac(int val)
{
  int ret = 1;
  for (int i = 1
; i <= val; ++i)
    ret *= i;

  return ret;
}

int main()
{
  for (int i = 0; i < 100; i += 7)
    {
      std::vector<int> digits = get_digits(i);

      for (int j = 0; j < digits.size(); ++j)
	std::cout << digits[j] << " ";
      std::cout << std::endl;
    }

  for (int i = 0; i < 10; ++i)


  return 0;
}
